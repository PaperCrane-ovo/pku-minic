decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @f(@a: i32): i32 {
%entry:
  %a = alloc i32
  store @a, %a
  %0 = load %a
  ret %0
}

fun @main(): i32 {
%entry:
  %1 = alloc i32
  store 1, %1
  %2 = load %1
  %3 = call @f(%2)
  ret %3
}
